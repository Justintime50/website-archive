{
  "name": "website-archive",
  "description": "A collection of early projects from my web developer career.",
  "version": "1.0.0",
  "devDependencies": {
    "htmlhint": "^1.1",
    "justintime50-styles": "0.4.0",
    "stylelint": "^15.6",
    "stylelint-config-standard": "^33.0"
  },
  "scripts": {
    "lint-html": "./node_modules/.bin/htmlhint src",
    "lint-css": "npx stylelint \"**/*.css\" --config node_modules/justintime50-styles/src/css/.stylelintrc.json",
    "fix-css": "npx stylelint \"**/*.css\" --config node_modules/justintime50-styles/src/css/.stylelintrc.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justintime50/website-archive.git"
  },
  "keywords": [
    "website",
    "archive"
  ],
  "author": "Justintime50",
  "bugs": {
    "url": "https://github.com/Justintime50/website-archive/issues"
  },
  "homepage": "https://github.com/Justintime50/website-archive#readme"
}
